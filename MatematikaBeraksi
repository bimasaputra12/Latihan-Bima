kelas publik Matematika {
    batal pertambahan (int a, int b) {
        System.out.println ("hasil =" + (a + b));
        
    }
    batal (int a, int b) {
        System.out.println ("hasil =" + (ab));
    }
    void perkalian (int a, double b) {
        System.out.println ("hasil =" + (a * b));
    }
    batal pembagian (float a, float b) {
        System.out.println ("hasil =" + (a / b));
    }
Parameter paket;

/ **
 *
 * @ penulis Farhan ashari
 * /
kelas publik Matematikaberaksi {
    public static void main (String [] args) {
        Matematika matematikaasik = Matematika baru ();
        
        matematikaasik.pertambahan (15,10);
        matematikaasik.pengurangan (25,5);
        matematikaasik.perkalian (5,8);
        matematikaasik.pembagian (50,2);
    }
}
